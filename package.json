{
  "name": "cumc",
  "version": "1.0.0",
  "description": "Website of the Cambridge University Mountaineering Club",
  "main": "./server/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cd server && npm start",
    "dev": "./dev.sh",
    "dev:internal": "concurrently --kill-others-on-fail -n \"SERVER,CLIENT\" -c \"blue,green\" \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && BROWSER=none npm start",
    "heroku-postbuild": "cd server && npm install && cd ../client && npm install && npm run build"
  },
  "keywords": [],
  "author": "Edmund Ross",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
