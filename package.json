{
  "name": "cumc",
  "version": "1.0.0",
  "description": "Website of the Cambridge University Mountaineering Club",
  "main": "./server/dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run start:prod",
    "start:prod": "cd server && node dist/app.js",
    "start:dev": "cd server && npm start",
    "dev": "./dev.sh",
    "dev:internal": "concurrently --kill-others-on-fail -n \"SERVER,CLIENT\" -c \"blue,green\" \"npm run server:dev\" \"npm run client\"",
    "server": "npm run server:dev",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "client": "cd client && BROWSER=none npm start",
    "client:build": "cd client && npm run build",
    "build": "npm run server:build && npm run client:build",
    "build:server": "npm run server:build",
    "build:client": "npm run client:build",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "heroku-postbuild": "npm run install:all && npm run build"
  },
  "keywords": [],
  "author": "Edmund Ross",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
